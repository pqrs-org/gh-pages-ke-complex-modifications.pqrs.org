{
  "title": "Nightfall Dark Mode Toggle",
  "rules": [
    {
      "description": "Change F7 to Ctrl+Cmd+Opt+T (toggle dark mode from Nightfall)",
      "manipulators": [
        {
          "from": {
            "key_code": "f7"
          },
          "to": [
            {
              "key_code": "t",
              "modifiers": [
                "left_control",
                "left_command",
                "left_option"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
