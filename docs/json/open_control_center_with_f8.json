{
  "title": "Launch Control Center Using \"f8\"",
  "rules": [
    {
      "description": "Map f8 button to fn+c",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f8"
          },
          "to": [
            {
              "repeat": false,
              "key_code": "c",
              "modifiers": [
                "fn"
              ]
            }
          ]
        }
      ]
    }
  ]
}