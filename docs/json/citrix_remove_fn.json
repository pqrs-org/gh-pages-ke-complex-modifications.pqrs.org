{
    "title": "Citrix Receiver/Workspace remove Fn key from Functional (F*) keys",
    "rules": [
        {
            "description": "Remove Fn from F* keys in Citrix",
            "manipulators": [
                {
                    "conditions": [
                        {
                            "bundle_identifiers": [
                                "^com\\.citrix\\.XenAppViewer$",
                                "^com\\.citrix\\.receiver\\.icaviewer\\.mac$"
                            ],
                            "type": "frontmost_application_if"
                        }
                    ],
                    "from": {
                        "key_code": "f1"
                    },
