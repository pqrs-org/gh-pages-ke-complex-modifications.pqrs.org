{
    "title": "Maps Screenshot to Backspace (Keychron K3)",
    "rules": [
        {
            "description": "Screenshot to Backspace",
            "manipulators": [
                {
                    "from": {
                        "key_code": "4",
                        "modifiers": {
                            "mandatory": [
                                "left_command",
                                "left_shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "delete_or_backspace"
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
    ]
}
