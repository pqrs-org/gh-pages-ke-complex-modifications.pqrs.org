{
	"title": "Super ctrl + cmd!",
		"rules": [
		{
			"description": "ctrl + S を option + ← に変換",
			"manipulators": [
			{
				"type": "basic",
				"from": {
					"key_code": "s",
					"modifiers": {
						"mandatory": ["control"]
					}
				},
				"to": [
				{
					"key_code": "left_arrow",
					"modifiers": ["option"]
				}
				],
				"conditions": [
				{
					"type": "frontmost_application_unless",
					"bundle_identifiers": [
						"com.microsoft.VSCode",
					"com.apple.Terminal",
					"com.googlecode.iterm2",
					"net.kovidgoyal.kitty",
					"com.github.wez.wezterm"
					]
				}
				]
			}
			]
		},
		{
			"description": "ctrl + G を option + → に変換",
