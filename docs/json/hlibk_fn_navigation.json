{
  "title": "Hlibk Fn navigation",
  "rules": [
    {
      "description": "FN + H to Left Arrow",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": ["caps_lock", "option"]
            }
          },
          "to": [
            {
              "key_code": "left_arrow"
            }
          ]
        }
      ]
    },
    {
      "description": "FN + J to Down Arrow",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": ["caps_lock", "option"]
            }
          },
          "to": [
            {
              "key_code": "down_arrow"
            }
          ]
        }
      ]
    },
    {
      "description": "FN + K to Up Arrow",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": ["caps_lock", "option"]
            }
          },
          "to": [
            {
              "key_code": "up_arrow"
            }
          ]
        }
      ]
    },
    {
      "description": "FN + L to Right Arrow",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": ["caps_lock", "option"]
            }
          },
          "to": [
            {
              "key_code": "right_arrow"
            }
          ]
        }
      ]
    },
    {
      "description": "FN + I to Command + Left Arrow",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "i",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": ["caps_lock", "option"]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": ["command"]
            }
          ]
        }
      ]
    },
    {
      "description": "FN + O to Command + Right Arrow",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "o",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": ["caps_lock", "option"]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": ["command"]
            }
          ]
        }
      ]
    },
    {
      "description": "FN + < to Option + Left Arrow",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "comma",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": ["caps_lock"]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": ["option"]
            }
          ]
        }
      ]
    },
    {
      "description": "FN + > to Option + Right Arrow",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "period",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": ["caps_lock"]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": ["option"]
            }
          ]
        }
      ]
    },
    {
      "description": "FN + Y to Page Up",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "y",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": ["caps_lock", "option"]
            }
          },
          "to": [
            {
              "key_code": "page_up"
            }
          ]
        }
      ]
    },
    {
      "description": "FN + U to Page Down",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "u",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": ["caps_lock", "option"]
            }
          },
          "to": [
            {
              "key_code": "page_down"
            }
          ]
        }
      ]
    },
    {
      "description": "FN + P to Delete ot Backspace",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "p",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": ["caps_lock", "option"]
            }
          },
          "to": [
            {
              "key_code": "delete_or_backspace"
            }
          ]
        }
      ]
    },
    {
      "description": "FN + M to Delete Forward",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "m",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": ["caps_lock", "option"]
            }
          },
          "to": [
            {
              "key_code": "delete_forward"
            }
          ]
        }
      ]
    }
  ]
}
