{
  "title": "ISO PT-PT keyboard",
  "rules": [
    {
      "description": "Backslash",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "grave_accent_and_tilde"
          },
          "to": [
            {
              "key_code": "backslash"
            }
          ]
        }
      ]
    },
    {
      "description": "Pipe (left shift)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "grave_accent_and_tilde",
            "modifiers": {
              "mandatory": [
                "left_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "backslash",
              "modifiers": ["left_shift"]
            }
          ]
        }
      ]
    },
        {
      "description": "Pipe (right shift)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "grave_accent_and_tilde",
            "modifiers": {
              "mandatory": [
                "right_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "backslash",
              "modifiers": ["right_shift"]
            }
          ]
        }
      ]
    },
    {
      "description": "Tilde",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "non_us_pound"
          },
          "to": [
            {
              "key_code": "quote"
            }
          ]
        }
      ]
    },
    {
      "description": "Circumflex (right shift)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "non_us_pound",
            "modifiers": {
              "mandatory": [
                "right_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "quote",
              "modifiers": ["right_shift"]
            }
          ]
        }
      ]
    },
        {
      "description": "Circumflex (left shift)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "non_us_pound",
            "modifiers": {
              "mandatory": [
                "left_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "quote",
              "modifiers": ["left_shift"]
            }
          ]
        }
      ]
    },
    {
      "description": "Plus",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "open_bracket"
          },
          "to": [
            {
              "key_code": "equal_sign"
            }
          ]
        }
      ]
    },
    {
      "description": "Times (right shift)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "open_bracket",
            "modifiers": {
              "mandatory": [
                "right_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "equal_sign",
              "modifiers": ["right_shift"]
            }
          ]
        }
      ]
    },
    {
      "description": "Times (left shift)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "open_bracket",
            "modifiers": {
              "mandatory": [
                "left_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "equal_sign",
              "modifiers": ["left_shift"]
            }
          ]
        }
      ]
    },
    {
      "description": "Male ordinal indicator",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "quote"
          },
          "to": [
            {
              "key_code": "open_bracket"
            }
          ]
        }
      ]
    },
    {
      "description": "Female ordinal indicator (right shift)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "quote",
            "modifiers": {
              "mandatory": [
                "right_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "open_bracket",
              "modifiers": ["right_shift"]
            }
          ]
        }
      ]
    },
    {
      "description": "Female ordinal indicator (left shift)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "quote",
            "modifiers": {
              "mandatory": [
                "left_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "open_bracket",
              "modifiers": ["left_shift"]
            }
          ]
        }
      ]
    },
    {
      "description": "Right curly bracket",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "modifiers": {
              "mandatory": [
                "right_option"
              ]
            },
            "key_code": "0"
          },
          "to": [
            {
              "key_code": "9",
              "modifiers": [
                "left_option","left_shift"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Left curly bracket",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "modifiers": {
              "mandatory": [
                "right_option"
              ]
            },
            "key_code": "7"
          },
          "to": [
            {              
              "modifiers": [
                "left_option","left_shift"
              ],
              "key_code": "8"
            }
          ]
        }
      ]
    },
    {
      "description": "Guillemets",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "equal_sign"
          },
          "to": [
            {              
              "modifiers": [
                "left_option"
              ],
              "key_code": "x"
            }
          ]
        }
      ]
    },
    {
      "description": "Guillemets (left shift)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "modifiers": {
              "mandatory": [
                "left_shift"
              ]
            },
            "key_code": "equal_sign"
          },
          "to": [
            {              
              "modifiers": [
                "left_option","left_shift"
              ],
              "key_code": "x"
            }
          ]
        }
      ]
    },
    {
      "description": "Guillemets (right shift)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "modifiers": {
              "mandatory": [
                "right_shift"
              ]
            },
            "key_code": "equal_sign"
          },
          "to": [
            {              
              "modifiers": [
                "left_option","left_shift"
              ],
              "key_code": "x"
            }
          ]
        }
      ]
    }
  ]
}