{
  "title": "Microsoft Sculpt Ergonomic Desktop (latin american keyboard)",
  "rules": [
    {
      "description": "Swap 'left Alt ‚éá' key and 'Windows key'",
      "manipulators": [
        {
          "from": {"key_code": "left_option","modifiers": {"optional": ["any"]}},
          "to": [{"key_code": "left_command"}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}],
          "type": "basic"
        },
        {
          "from": {"key_code": "left_command","modifiers": {"optional": ["any"]}},
          "to": [{"key_code": "left_option"}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Swap 'right Alt ‚éá' key and 'Menu/Application' key",
      "manipulators": [
        {
          "from": {"key_code": "right_option","modifiers": {"optional": ["any"]}},
          "to": [{"key_code": "right_command"}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}],
          "type": "basic"
        },
        {
          "from": {"key_code": "application","modifiers": {"optional": ["any"]}},
          "to": [{"key_code": "right_option"}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Map 'Home' key to '‚åò + Left arrow'",
      "manipulators": [
        {
          "from": {"key_code": "home"},
          "to": [{"key_code": "left_arrow","modifiers": "command"}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Map 'End' key to '‚åò + Right arrow'",
      "manipulators": [
        {
          "from": {"key_code": "end"},
          "to": [{"key_code": "right_arrow","modifiers": "command"}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Map 'PrintScreen' key to 'ScreenShoot'",
      "manipulators": [
        {
          "from": {"key_code": "print_screen"},
          "to": [{"shell_command": "screencapture -i -W -U -c"}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Map 'ScrLk' key to 'Eject'",
      "manipulators": [
        {
          "from": {"key_code": "scroll_lock","modifiers": {"optional": ["any"]}},
          "to": [{"key_code": "eject"}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Map 'Pause' key to 'Fn' key",
      "manipulators": [
        {
          "from": {"key_code": "pause"},
          "to": [{"key_code": "fn"}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Map 'Task View' key (F2 key) to launching 'Mission Control.app'",
      "manipulators": [
        {
          "from": {
            "key_code": "tab",
            "modifiers": {
              "mandatory": ["left_option"],
              "optional": ["caps_lock"]
            }
          },
          "to": [{"apple_vendor_keyboard_key_code": "mission_control"}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Map 'Desktop Left' key (F3 key) to 'Move left a space' action",
      "manipulators": [
        {
          "from": {
            "key_code": "left_arrow",
            "modifiers": {
              "mandatory": [
                "left_option",
                "left_control"
              ],
              "optional": ["caps_lock"]
            }
          },
          "to": [{"key_code": "left_arrow","modifiers": ["left_control"]}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Map 'Desktop Right' key (F4 key) to 'Move right a space' action",
      "manipulators": [
        {
          "from": {
            "key_code": "right_arrow",
            "modifiers": {
              "mandatory": [
                "left_option",
                "left_control"
              ],
              "optional": ["caps_lock"]
            }
          },
          "to": [{
            "key_code": "right_arrow",
            "modifiers": ["left_control"]
          }],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Map 'Cortana/Search' key (F5 key) to launching 'Spotlight'",
      "manipulators": [
        {
          "from": {
            "key_code": "f21",
            "modifiers": {
              "mandatory": [
                "left_option",
                "left_shift"
              ],
              "optional": ["caps_lock"]
            }
          },
          "to": [{"apple_vendor_keyboard_key_code": "spotlight"}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Map 'Network' key (F6 key) to launching 'Safari.app'",
      "manipulators": [
        {
          "from": {
            "key_code": "f21",
            "modifiers": {
              "mandatory": [
                "left_option",
                "left_command"
              ],
              "optional": ["caps_lock"]
            }
          },
          "to": [{"shell_command": "open -b com.apple.safari"}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Map 'Connect' key (F7 key) to launching 'Screen Sharing.app'",
      "manipulators": [
        {
          "from": {
            "key_code": "f21",
            "modifiers": {
              "mandatory": [
                "left_option",
                "left_control"
              ],
              "optional": ["caps_lock"]
            }
          },
          "to": [{"shell_command": "open -a 'Screen Sharing.app'"}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Map 'Windows Settings' key (F8 key) to launching 'System Preferences.app'",
      "manipulators": [
        {
          "from": {
            "key_code": "f21",
            "modifiers": {
              "mandatory": ["left_option"],
              "optional": ["caps_lock"]
            }
          },
          "to": [{"shell_command": "open -b com.apple.systempreferences"}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}],
          "type": "basic"
        }
      ]
    },
    {
    "description": "Map 'Back' mouse button to 'quote + left_command' action",
    "manipulators": [
    {
          "from": {
            "pointing_button": "button4",
            "modifiers": {"optional": ["any"]}
          },
          "to": [{"key_code": "quote", "modifiers": "left_command"}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Map 'Windows' mouse button to launching 'Launchpad.app'",
      "manipulators": [
        {
          "from": {
            "key_code": "right_gui",
            "modifiers": {"optional": ["any"]}
          },
          "to": [{"shell_command": "open -a 'Launchpad.app'"}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Toggle 'NumLock' and map action keys on Numpad",
      "manipulators": [
        {
          "description": "Enable 'NumLock'",
          "from": {
            "key_code": "keypad_num_lock",
            "modifiers": { "optional": ["any"] }
          },
          "to": [
            {"set_variable": {"name": "NumLock","value":1}},
            {"shell_command": "osascript -e 'display notification \"üî¢ Numeric keys are now active.\" with title \"NumLock enabled\" sound name \"Tink\"'"}
          ],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]},{"type": "variable_if","name": "NumLock","value":0}],
          "type": "basic"
        },
        {
          "description": "Disable 'NumLock'",
          "from": {
            "key_code": "keypad_num_lock",
            "modifiers": { "optional": ["any"] }
          },
          "to": [
            {"set_variable": {"name": "NumLock","value":0}},
            {"shell_command": "osascript -e 'display notification \"‚ÜîÔ∏è Actions keys are now active.\" with title \"NumLock disabled\" sound name \"Tink\"'"}
          ],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]},{"type": "variable_unless","name": "NumLock","value":0}],
          "type": "basic"
        },
        {
          "description": "Execute 'Insert' with 'keypad 0'",
          "from": {
            "key_code": "keypad_0",
            "modifiers": { "optional": ["any"] }
          },
          "to": [{"key_code": "insert"}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}, {"type": "variable_unless","name": "NumLock","value":1}],
          "type": "basic"
        },
        {
          "description": "Execute 'End' with 'keypad 1'",
          "from": {
            "key_code": "keypad_1",
            "modifiers": { "optional": ["any"] }
          },
          "to": [{"key_code": "end"}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}, {"type": "variable_unless","name": "NumLock","value":1}],
          "type": "basic"
        },
        {
          "description": "Execute 'Down arrow' with 'keypad 2'",
          "from": {
            "key_code": "keypad_2",
            "modifiers": { "optional": ["any"] }
          },
          "to": [{"key_code": "down_arrow"}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}, {"type": "variable_unless","name": "NumLock","value":1}],
          "type": "basic"
        },
        {
          "description": "Execute 'Page down' with 'keypad 3'",
          "from": {
            "key_code": "keypad_3",
            "modifiers": { "optional": ["any"] }
          },
          "to": [{"key_code": "page_down"}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}, {"type": "variable_unless","name": "NumLock","value":1}],
          "type": "basic"
        },
        {
          "description": "Execute 'Left arrow' with 'keypad 4'",
          "from": {
            "key_code": "keypad_4",
            "modifiers": { "optional": ["any"] }
          },
          "to": [{"key_code": "left_arrow"}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}, {"type": "variable_unless","name": "NumLock","value":1}],
          "type": "basic"
        },
        {
          "description": "Disable 'keypad 5'",
          "from": {
            "key_code": "keypad_5"
          },
          "to": [{"key_code": "vk_none"}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}, {"type": "variable_unless","name": "NumLock","value":1}],
          "type": "basic"
        },
        {
          "description": "Execute 'Right arrow' with 'keypad 6'",
          "from": {
            "key_code": "keypad_6",
            "modifiers": { "optional": ["any"] }
          },
          "to": [{"key_code": "right_arrow"}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}, {"type": "variable_unless","name": "NumLock","value":1}],
          "type": "basic"
        },
        {
          "description": "Execute 'Home' with 'keypad 7'",
          "from": {
            "key_code": "keypad_7",
            "modifiers": { "optional": ["any"] }
          },
          "to": [{"key_code": "home"}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}, {"type": "variable_unless","name": "NumLock","value":1}],
          "type": "basic"
        },
        {
          "description": "Execute 'Up arrow' with 'keypad 8'",
          "from": {
            "key_code": "keypad_8",
            "modifiers": { "optional": ["any"] }
          },
          "to": [{"key_code": "up_arrow"}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}, {"type": "variable_unless","name": "NumLock","value":1}],
          "type": "basic"
        },
        {
          "description": "Execute 'Page up' with 'keypad 9'",
          "from": {
            "key_code": "keypad_9",
            "modifiers": { "optional": ["any"] }
          },
          "to": [{"key_code": "page_up"}],
          "conditions": [{"type": "device_if","identifiers": [{"vendor_id":1118,"product_id":1957}]}, {"type": "variable_unless","name": "NumLock","value":1}],
          "type": "basic"
        }
      ]
    }
  ]
}
