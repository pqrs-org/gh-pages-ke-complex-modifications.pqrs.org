    "rules": [
        {
            "description": "Y => Z (German layout)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "y"
                    },
                    "to": [
                        {
                            "repeat": false,
                            "key_code": "z"
                        }
                    ]
                }
            ]
        },
        {
            "description": "Z => Y (German layout)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "z"
                    },
                    "to": [
                        {
                            "repeat": false,
                            "key_code": "y"
                        }
                    ]
                }
            ]
        },
        {
            "description": "Ctrl+Y => Cmd+Shift+Y (Redo) (German layout)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "modifiers": {
                            "mandatory": [
                                "left_control"
                            ]
                        },
                        "key_code": "z"
                    },
                    "to": [
                        {
                            "repeat": false,
                            "key_code": "y",
                            "modifiers": [
                                "left_shift",
                                "left_command"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_unless",
                            "bundle_identifiers": [
                                "^com\\.microsoft\\.rdc$",
                                "^com\\.microsoft\\.rdc\\.",
                                "^net\\.sf\\.cord$",
                                "^com\\.thinomenon\\.RemoteDesktopConnection$",
                                "^com\\.itap-mobile\\.qmote$",
                                "^com\\.nulana\\.remotixmac$",
                                "^com\\.p5sys\\.jump\\.mac\\.viewer$",
                                "^com\\.p5sys\\.jump\\.mac\\.viewer\\.",
                                "^com\\.teamviewer\\.TeamViewer$",
                                "^com\\.philandro\\.anydesk$",
                                "^com\\.vmware\\.horizon$",
                                "^com\\.2X\\.Client\\.Mac$",
                                "^com\\.OpenText\\.Exceed-TurboX-Client$",
                                "^com\\.realvnc\\.vncviewer$",
                                "^com\\.citrix\\.receiver\\.icaviewer",
                                "^com\\.vmware\\.fusion$",
                                "^com\\.vmware\\.horizon$",
                                "^com\\.vmware\\.view$",
                                "^com\\.parallels\\.desktop$",
                                "^com\\.parallels\\.vm$",
                                "^com\\.parallels\\.desktop\\.console$",
