{
  "title": "Map fn+up to down",
  "rules": [
    {
      "description": "fn + up to down",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "up_arrow",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "down_arrow"
            }
          ]
        }
      ]
    }
  ]
}
