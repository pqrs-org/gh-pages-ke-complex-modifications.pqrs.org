{
    "title": "Swap z to [copy] & x to [paste] for 2 keypad keyboard like osu nono",
    "maintainers": [
      "goldsky"
    ],
    "rules": [
      {
        "description": "Swap z to [copy] & x to [paste] for 2 keypad keyboard like osu nono",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "z"
            },
            "to": [
              {
                "repeat": false,
                "modifiers": [
                  "left_gui",
                  "right_gui"
                ],
                "key_code": "c"
              }
            ]
          },
          {
            "type": "basic",
            "from": {
              "key_code": "x"
            },
            "to": [
              {
                "repeat": false,
                "modifiers": [
                  "left_gui",
                  "right_gui"
                ],
                "key_code": "v"
              }
            ]
          }
        ]
      }
    ]
  }