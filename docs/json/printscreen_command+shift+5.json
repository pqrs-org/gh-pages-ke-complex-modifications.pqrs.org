{
  "title": "<Macos> Simple PrintScreen to Command+Shift+5",
  "rules": [
    {
      "description": "Print Screen to Command+Shift+5",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "print_screen",
            "modifiers": {
              "optional": [ "caps_lock" ]
            }
          },
          "to": {
            "key_code": "5",
            "modifiers": [
              "left_command",
              "left_shift"
            ]
          }
        }
      ]
    }
  ]
}
