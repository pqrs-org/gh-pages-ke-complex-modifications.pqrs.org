{
  "title": "Map Right Option Key to Underscore",
  "rules": [
    {
      "description": "Map Right Option Key to Underscore",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "right_option",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "hyphen",
              "modifiers": ["left_shift"]
            }
          ]
        }
      ]
    }
  ]
}
