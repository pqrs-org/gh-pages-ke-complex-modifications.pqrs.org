{
  "title": "Danish DK Programmer Keyboard",
  "rules": [
    {
      "description": "Danish DK keyboard, Right Alt + 7,0 =>            { , }",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "7",
            "modifiers": {
              "mandatory": ["right_alt"]
            }
          },
          "to": [
            {
              "key_code": "8",
              "modifiers": ["left_option", "left_shift"]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "0",
            "modifiers": {
              "mandatory": ["right_alt"]
            }
          },
          "to": [
            {
              "key_code": "9",
              "modifiers": ["left_option", "left_shift"]
            }
          ]
        }
      ]
    },
    {
      "description": "Danish dk keyboard, AltGr+key_left_of_backspace =>        pipe    |",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "equal_sign",
            "modifiers": {
              "mandatory": ["right_option"]
            }
          },
          "to": [
            {
              "key_code": "7",
              "modifiers": ["right_option"]
            }
          ]
        }
      ]
    },
    {
      "description": "Danish Dk keyboard, AltGr+key_left_of_z =>          Backslash \\",
      "manipulators": [
        {
          "from": {
            "key_code": "non_us_backslash",
            "modifiers": {
              "mandatory": ["right_option"]
            }
          },
          "to": [
            {
              "key_code": "7",
              "modifiers": ["left_option", "left_shift"]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
    "description": "Danish Dk keyboard, key_left_of_z =>         <",
    "manipulators": [
        {
            "from": {
                "key_code": "non_us_backslash",
                "modifiers": {
                    "mandatory": []
                }
            },
            "to": [
                {
                    "key_code": "grave_accent_and_tilde",
                    "modifiers": []
                }
          ],
          "type": "basic"
        }
      ]
    },
    {
    "description": "Danish Dk keyboard, shift key_left_of_z =>         >",
    "manipulators": [
        {
            "from": {
                "key_code": "non_us_backslash",
                "modifiers": {
                    "mandatory": [
                        "left_shift"
                    ]
                }
            },
            "to": [
                {
                    "key_code": "grave_accent_and_tilde",
                    "modifiers": [
                        "left_shift"
                    ]
                }
          ],
          "type": "basic"
        }
      ]
    }
    
  ]
}
