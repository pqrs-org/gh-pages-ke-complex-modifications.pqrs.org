    },
    {
      "description": "Left_Command alone -> EISUU, Right_Command alone -> KANA",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_command",
            "modifiers": {
              "optional": [
                "shift",
                "option",
                "control",
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_command"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "japanese_eisuu"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "right_command",
            "modifiers": {
              "optional": [
                "shift",
                "option",
                "control",
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_command"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "japanese_kana"
            }
          ]
        }
      ]
    },
    {
      "description": "Command + Q * 2 -> Command + Q",
      "manipulators": [
        {
          "type": "basic",
          "conditions": [
            {
              "type": "variable_if",
              "name": "command-q",
              "value": 1
            }
          ],
          "from": {
            "key_code": "q",
            "modifiers": {
              "mandatory": ["command"],
              "optional": ["caps_lock"]
            }
          },
          "to": [
            {
              "key_code": "q",
              "modifiers": "left_command"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "q",
            "modifiers": {
              "mandatory": ["command"],
              "optional": ["caps_lock"]
            }
          },
          "to": [
            {
              "set_variable": {
                "name": "command-q",
                "value": 1
              }
            }
          ],
          "to_delayed_action": {
            "to_if_invoked": [
              {
                "set_variable": {
                  "name": "command-q",
                  "value": 0
                }
              }
            ],
            "to_if_canceled": [
              {
                "set_variable": {
                  "name": "command-q",
                  "value": 0
                }
              }
            ]
          }
        }
      ]
    }
  ]
}
