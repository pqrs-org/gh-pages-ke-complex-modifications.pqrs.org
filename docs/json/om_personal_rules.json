{
  "title": "Personal Rules (@omonomo)",
  "rules": [
    {
      "description": "Fn + Esc -> Clear",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "escape",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "keypad_num_lock"
            }
          ]
        }
      ]
    },
    {
      "description": "Fn + Caps_Lock -> Caps_Lock, Caps_Lock -> Right_Control",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "optional": [
                "shift",
                "option",
                "control",
                "command",
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_control"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "right_control",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": ["caps_lock"]
            }
          },
          "to": [
            {
              "key_code": "caps_lock"
            }
          ]
        }
      ]
    },
    {
      "description": "Right_Control + Shift + Space -> Insert Space",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": [
              "right_control",
              "shift"
              ],
              "optional": ["caps_lock"]
            }
          },
          "to": [
            {
              "key_code": "spacebar"
            },
            {
              "key_code": "left_arrow",
              "repeat": false
            }
          ]
        }
      ]
    },
    {
      "description": "Fn + Space -> Space (repeat), Space alone -> Space, Space hold -> Left_Shift",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "optional": [
                "shift",
                "option",
                "control",
                "command",
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_shift"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "spacebar"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "spacebar"
            }
          ]
        }
      ]
    },
    {
      "description": "Fn + Return -> Enter, Return alone -> Return, Return hold -> Left_Control",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "return_or_enter",
            "modifiers": {
              "optional": [
                "shift",
                "option",
                "control",
                "command",
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_control"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "return_or_enter"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "return_or_enter",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "keypad_enter"
            }
          ]
        }
      ]
    },
    {
      "description": "Right_Control + Left_Option + E/X/S/D -> Left_Control + Left_Option + Up/Down/Left/Right",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "e",
            "modifiers": {
              "mandatory": [
                "right_control",
                "left_option"
              ],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "up_arrow",
              "modifiers": [
                "left_option",
                "left_control"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "x",
            "modifiers": {
              "mandatory": [
                "right_control",
                "left_option"
              ],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "down_arrow",
              "modifiers": [
                "left_option",
                "left_control"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "s",
            "modifiers": {
              "mandatory": [
                "right_control",
                "left_option"
              ],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": [
                "left_option",
                "left_control"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "d",
            "modifiers": {
              "mandatory": [
                "right_control",
                "left_option"
              ],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": [
                "left_option",
                "left_control"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Right_Control + Up/Down/Left/Right -> Up/Down/Left/Right",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "up_arrow",
            "modifiers": {
              "mandatory": ["right_control"],
              "optional": ["caps_lock"]
            }
          },
          "to": [{"key_code": "up_arrow"}]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "down_arrow",
            "modifiers": {
              "mandatory": ["right_control"],
              "optional": ["caps_lock"]
            }
          },
          "to": [{"key_code": "down_arrow"}]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "left_arrow",
            "modifiers": {
              "mandatory": ["right_control"],
              "optional": ["caps_lock"]
            }
          },
          "to": [{"key_code": "left_arrow"}]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "right_arrow",
            "modifiers": {
              "mandatory": ["right_control"],
              "optional": ["caps_lock"]
            }
          },
          "to": [{"key_code": "right_arrow"}]
        }
      ]
    },
    {
      "description": "Right_Control + E/X/S/D/R/C -> Up/Down/Left/Right/PageUp/PageDown",
      "manipulators": [
        {
          "type": "basic",
