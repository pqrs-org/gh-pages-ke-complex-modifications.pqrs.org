        },
        {
          "type": "basic",
          "from": {
            "key_code": "s",
            "modifiers": {
              "mandatory": ["right_control"],
              "optional": ["any"]
            }
          },
          "to": [{"key_code": "left_arrow"}]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "d",
            "modifiers": {
              "mandatory": ["right_control"],
              "optional": ["any"]
            }
          },
          "to": [{"key_code": "right_arrow"}]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "r",
            "modifiers": {
              "mandatory": ["right_control"],
              "optional": ["any"]
            }
          },
          "to": [{"key_code": "page_up"}]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "c",
            "modifiers": {
              "mandatory": ["right_control"],
              "optional": ["any"]
            }
          },
          "to": [{"key_code": "page_down"}]
        }
      ]
    },
    {
      "description": "Right_Control + A/F -> Left_Option + Left/Right",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "a",
            "modifiers": {
              "mandatory": ["right_control"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": ["left_option"]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f",
            "modifiers": {
              "mandatory": ["right_control"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": ["left_option"]
            }
          ]
        }
      ]
    },
    {
      "description": "Right_Control + U/I/M/V/H/G -> Esc/Tab/Return/Enter/Delete/Forward_Delete",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "u",
            "modifiers": {
              "mandatory": ["right_control"],
              "optional": ["any"]
            }
          },
          "to": [{"key_code": "escape"}]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "i",
            "modifiers": {
              "mandatory": ["right_control"],
              "optional": ["any"]
            }
          },
          "to": [{"key_code": "tab"}]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "m",
            "modifiers": {
              "mandatory": ["right_control"],
              "optional": ["any"]
            }
          },
          "to": [{"key_code": "return_or_enter"}]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "v",
            "modifiers": {
              "mandatory": ["right_control"],
              "optional": ["any"]
            }
          },
          "to": [{"key_code": "keypad_enter"}]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": ["right_control"],
              "optional": ["any"]
            }
          },
          "to": [{"key_code": "delete_or_backspace"}]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "g",
            "modifiers": {
              "mandatory": ["right_control"],
              "optional": ["any"]
            }
          },
          "to": [{"key_code": "delete_forward"}]
        }
      ]
    },
    {
