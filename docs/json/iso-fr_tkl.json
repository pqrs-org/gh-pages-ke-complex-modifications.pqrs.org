{
    "title": "Complete layout for ISO-FR AZERTY keyboards (TKL Keyboard) + Basics Ctrl + [?] shortcuts",
    "rules": [
        {
            "description": "Tild (~) with right_option + é",
            "manipulators": [
                {
                    "from": {
                        "key_code": "2",
                        "modifiers": {
                            "mandatory": [
                                "right_option"
                            ]
                        }
                    },
                    "to": {
                        "key_code": "n",
                        "modifiers": [
                            "right_option"
                        ]
                    },
                    "type": "basic"
                }
            ]
        },
        {
            "description": "Hashtag (#) with right_option + “ ",
            "manipulators": [
                {
                    "from": {
                        "key_code": "3",
                        "modifiers": {
                            "mandatory": [
                                "right_option"
                            ]
                        }
                    },
                    "to": {
                        "key_code": "non_us_backslash",
                        "modifiers": [
                            "shift"
                        ]
                    },
                    "type": "basic"
                }
            ]
        },
        {
            "description": "Curly bracket ({) with  right_option + 4",
            "manipulators": [
                {
                    "from": {
                        "key_code": "4",
                        "modifiers": {
