{
    "title": "Spanish keys for english keyboard",
    "rules": [
        {
            "description": "Add n with sign",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "n",
                        "modifiers": {
                            "mandatory": ["control"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "n",
                            "modifiers": ["option"]
                        },
                        {
                            "key_code": "n"
                        }
                    ]
                }
            ]
        },
        {
            "description": "Add a with accent mark",
            "manipulators": [
                {
                    "type": "basic",
