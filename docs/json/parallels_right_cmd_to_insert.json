{
  "title": "Right command is insert in Parallels (for JAWS, NVDA, Narrator)",
  "rules": [
    {
      "description": "Right command is insert in Parallels",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "right_command",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "insert"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": ["^com\\.parallels\\..*"]
            }
          ]
        }
      ]
    }
  ]
}
