{
  "title": "Change shift+f19 to capslock",
  "rules": [
    {
      "description": "Change shift+f19 to capslock",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f19",
            "modifiers": {
              "mandatory": ["shift"]
            }
          },
          "to": [
            {
              "key_code": "caps_lock"
            }
          ]
        }
      ]
    }
  ]
}
