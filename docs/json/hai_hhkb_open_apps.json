{
	"title": "Hai HHKB Launch apps",
	"rules": [
		{
			"description": "right_option + B -> Brave",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "b",
						"modifiers": {
							"mandatory": ["right_option"]
						}
					},
					"to": [
						{
							"shell_command": "open '/System/Applications/Brave Browser.app'"
						}
					]
				}
			]
		},
		{
			"description": "right_option + E -> VS code",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "e",
						"modifiers": {
							"mandatory": ["right_option"]
						}
					},
					"to": [
						{
							"shell_command": "open '/Applications/Visual Studio Code.app'"
						}
					]
				}
			]
		},
		{
			"description": "right_option + S -> Signal",
			"manipulators": [
				{
					"type": "basic",
